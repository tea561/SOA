{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Data Service",
        "description": "Express.js service as Data Service"
    },
    "basePath": "/",
    "tags": [
        {
            "name": "Data Service"
        }
    ],
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/getVitals/{userID}": {
            "get": {
                "tags": [
                    "Data Service"
                ],
                "summary": "Get vital parameters for specific user",
                "parameters": [
                    {
                        "name": "userID",
                        "type": "integer",
                        "in": "path",
                        "description": "User ID",
                        "required": true
                    }
                ],
                "responses": {
                    "404": {
                        "description": "User not found."
                    },
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/postVitals": {
            "post": {
                "tags": [
                    "Data Service"
                ],
                "summary": "Post vital parameters for specific user.",
                "description": "Sample request:\r\n    \r\n    POST /Gateway\r\n    {\r\n        \"sys\": 120,\r\n        \"dias\": 80,\r\n        \"pulse\": 90,\r\n        \"timestamp\": 1652119779,\r\n        \"userID\": 3\r\n    }",
                "parameters": [
                    {
                        "in": "body",
                        "name": "vitals",
                        "description": "Vital parameters for a user.",
                        "schema": {
                            "type": "object",
                            "required": [
                                "sys",
                                "dias",
                                "pulse",
                                "userID",
                                "timestamp"
                            ],
                            "properties": {
                                "sys": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "dias": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "pulse": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "userID": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "timestamp": {
                                    "type": "integer",
                                    "format": "int32"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Post parameters not defined."
                    },
                    "200": {
                        "description": "Vitals successfully added."
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "/putVitals": {
            "put": {
                "tags": [
                    "Data Service"
                ],
                "summary": "Update vital parameters for specific user.",
                "description": "Sample request:\r\n    \r\n    POST /Gateway\r\n    {\r\n        \"sys\": 120,\r\n        \"dias\": 80,\r\n        \"pulse\": 90,\r\n        \"timestamp\": 1652119779,\r\n        \"userID\": 3\r\n    }",
                "parameters": [
                    {
                        "in": "body",
                        "name": "vitals",
                        "description": "Vital parameters for a user.",
                        "schema": {
                            "type": "object",
                            "required": [
                                "sys",
                                "dias",
                                "pulse",
                                "userID",
                                "timestamp"
                            ],
                            "properties": {
                                "sys": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "dias": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "pulse": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "userID": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "timestamp": {
                                    "type": "integer",
                                    "format": "int32"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Put parameters not defined."
                    },
                    "200": {
                        "description": "Vitals successfully updated."
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "/deleteVitals{userID}": {
            "delete": {
                "tags": [
                    "Data Service"
                ],
                "summary": "Delete vital parameters for specific user.",
                "parameters": [
                    {
                        "name": "userID",
                        "type": "integer",
                        "in": "path",
                        "description": "User ID",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Vitals successfully deleted."
                    }
                }
            }
        }
    }
}